{
  "name": "bun-svelte-pwa",
  "version": "1.0.0",
  "description": "TypeScript monorepo template for Svelte PWA with Bun runtime",
  "private": true,
  "workspaces": [
    "modules/*"
  ],
  "scripts": {
    "dev": "nx dev @bun-svelte-pwa/app",
    "build": "nx build @bun-svelte-pwa/app",
    "preview": "nx preview @bun-svelte-pwa/app",
    "test": "nx test @bun-svelte-pwa/app",
    "test:watch": "nx test:watch @bun-svelte-pwa/app",
    "test:ui": "nx test:ui @bun-svelte-pwa/app",
    "test:e2e": "nx test:e2e @bun-svelte-pwa/app",
    "test:e2e:ui": "nx test:e2e:ui @bun-svelte-pwa/app",
    "clean": "nx run-many -t clean",
    "type-check": "nx run-many -t type-check"
  },
  "devDependencies": {
    "@nx/workspace": "^22.5.0",
    "nx": "^22.5.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "npm@9.0.0"
}