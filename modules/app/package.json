    "@testing-library/svelte": "^5.3.1",
    "@tsconfig/svelte": "^5.0.2",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
